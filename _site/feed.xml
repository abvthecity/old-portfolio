&lt;script&gt;

function gexp(figure) {
	if(figure.className.indexOf(&quot;expanded&quot;) &gt; -1){
		figure.className = figure.className.replace(&quot;expanded&quot;, &quot;&quot;)
		figure.innerHTML = figure.innerHTML.replace(&quot;lores&quot;,&quot;thumb&quot;)
	}
	else {
		figure.className += &quot; expanded&quot;
		figure.innerHTML = figure.innerHTML.replace(&quot;thumb&quot;,&quot;lores&quot;)
	}
}

&lt;/script&gt;</description>
